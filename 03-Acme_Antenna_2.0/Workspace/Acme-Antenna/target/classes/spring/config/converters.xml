<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		
		<bean class="converters.CommentToStringConverter" />
		<bean class="converters.StringToCommentConverter" />
		<bean class="converters.ActorToStringConverter" />
		<bean class="converters.StringToActorConverter" />
		<bean class="converters.UserToStringConverter" />
		<bean class="converters.StringToUserConverter" />
		<bean class="converters.AuthorityToStringConverter" />
		<bean class="converters.StringToAuthorityConverter" />
		<bean class="converters.UserAccountToStringConverter" />
		<bean class="converters.StringToUserAccountConverter" />
		<bean class="converters.AdministratorToStringConverter" />
		<bean class="converters.StringToAdministratorConverter" />
		<bean class="converters.CreditCardToStringConverter" />
		<bean class="converters.StringToCreditCardConverter" />
		<bean class="converters.AntennaToStringConverter" />
		<bean class="converters.StringToAntennaConverter" />
		<bean class="converters.GpsCoordinateToStringConverter" />
		<bean class="converters.StringToGpsCoordinateConverter" />
		<bean class="converters.PlatformToStringConverter" />
		<bean class="converters.StringToPlatformConverter" />
		<bean class="converters.SatelliteToStringConverter" />
		<bean class="converters.StringToSatelliteConverter" />
		<bean class="converters.SubscriptionToStringConverter" />
		<bean class="converters.StringToSubscriptionConverter" />
		<bean class="converters.TutorialToStringConverter" />
		<bean class="converters.StringToTutorialConverter" />
		<bean class="converters.AgentToStringConverter" />
		<bean class="converters.StringToAgentConverter" />
		<bean class="converters.BannerToStringConverter" />
		<bean class="converters.StringToBannerConverter" />
		<bean class="converters.HandyworkerToStringConverter" />
		<bean class="converters.StringToHandyworkerConverter" />
		<bean class="converters.RequestToStringConverter" />
		<bean class="converters.StringToRequestConverter" />

	</util:list>
	
</beans>