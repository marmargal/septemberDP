<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<!-- Add your own converters using entries of the following form: <bean 
			class="converters.Class1ToClass2" /> -->

		<bean class="converters.VisaToStringConverter" />
		<bean class="converters.StringToVisaConverter" />
		<bean class="converters.ActorToStringConverter" />
		<bean class="converters.StringToActorConverter" />
		<bean class="converters.AdministratorToStringConverter" />
		<bean class="converters.StringToAdministratorConverter" />
		<bean class="converters.ReportToStringConverter" />
		<bean class="converters.StringToReportConverter" />
		<bean class="converters.StringToOfficerConverter" />
		<bean class="converters.OfficerToStringConverter" />
		<bean class="converters.PersonalSectionToStringConverter" />
		<bean class="converters.StringToPersonalSectionConverter" />
		<bean class="converters.ContactSectionToStringConverter" />
		<bean class="converters.StringToContactSectionConverter" />
		<bean class="converters.WorkSectionToStringConverter" />
		<bean class="converters.StringToWorkSectionConverter" />
		<bean class="converters.SocialSectionToStringConverter" />
		<bean class="converters.StringToSocialSectionConverter" />
		<bean class="converters.EducationSectionToStringConverter" />
		<bean class="converters.StringToEducationSectionConverter" />
		<bean class="converters.CreditCardToStringConverter" />
		<bean class="converters.StringToCreditCardConverter" />
		<bean class="converters.AuthorityToStringConverter" />
		<bean class="converters.StringToAuthorityConverter" />
		<bean class="converters.UserAccountToStringConverter" />
		<bean class="converters.StringToUserAccountConverter" />
		<bean class="converters.LawToStringConverter" />
		<bean class="converters.StringToLawConverter" />
		<bean class="converters.DecisionToStringConverter" />
		<bean class="converters.StringToDecisionConverter" />

	</util:list>

</beans>